{
  "particles":{
    "number":{
      "value":60
    },
    "color":{
      "value": "#fff"
    },
    "shape":{
      "type":"circle"
    },
    "opacity":{
      "value":1,
      "random":true,
      "anim":{
        "enable":true,
        "speed":1
      }
    },
    "size":{
      "value": 5,
      "random": true
    },
    "move": {
      "enable": true,
      "speed": 0.5,
      "direction": "none",
      "random": true,
      "straight": false,
      "out_mode": "bounce",
      "bounce": true,
      "attract": {
        "enable": false,
        "rotateX": 600,
        "rotateY": 1200
      }
    }
  },
  "interactivity": {
    "detect_on": "canvas",
    "events": {
      "onhover": {
        "enable": true,
        "mode": "repulse"
      },
      "onclick": {
        "enable": true,
        "mode": "push"
      },
      "resize": true
    },
    "modes": {
      "grab": {
        "distance": 30,
        "line_linked": {
          "opacity": 1
        }
      },
      "bubble": {
        "distance": 20,
        "size": 10,
        "duration": 2,
        "opacity": 0.8,
        "speed": 3
      },
      "repulse": {
        "distance": 50,
        "duration": 0.4
      },
      "push": {
        "particles_nb": 4
      },
      "remove": {
        "particles_nb": 2
      }
    }
  },
  "retina_detect": true
}